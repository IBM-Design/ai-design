{
  "name": "gatsby-theme-carbon-starter",
  "version": "0.0.5",
  "main": "src",
  "repository": {
    "url": "git@github.com:carbon-design-system/gatsby-theme-carbon.git",
    "type": "git"
  },
  "author": "vpicone <vp@vincepic.one> (@vpicone)",
  "license": "MIT",
  "scripts": {
    "develop": "gatsby develop",
    "build": "gatsby clean && PATH_PREFIX='/design/ai' gatsby build --prefix-paths && mv public ai && mkdir design && mv ai design && mkdir public && mv design public && cp ./static/hero.png ./static/hero.mp4 public/design/ai",
    "develop:clean": "gatsby clean && gatsby develop",
    "prod": "gatsby clean && gatsby serve",
    "now-build": "npm run build"
  },
  "dependencies": {
    "gatsby": "^5.2.0",
    "gatsby-theme-carbon": "^1.27.0",
    "react": "^16.8.6",
    "react-dom": "^16.8.6"
  }
}
